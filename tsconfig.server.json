    "module": "CommonJS",
    "rootDir": "./server",
    "outDir": "./dist/server",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "include": ["server", "shared"]
}
